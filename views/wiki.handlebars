<link href="https://fonts.googleapis.com/css?family=Petit+Formal+Script|Quintessential|Dancing+Script|IBM+Plex+Mono|Gugi" rel="stylesheet">
<main>

<section id="results">
  {{#each Wikiarticle}}
  <article class="">
    <div class="demo-card-wide mdl-card mdl-shadow--2dp">
  <div class="mdl-card__title">
    <img  src={{img}} style="height:120%; width:100%;"/>
    <h2 class="mdl-card__title-text"></h2>
  </div>
  <div class="mdl-card__supporting-text">
    {{title}}
  </div>
  <div class="wiki-link mdl-card__actions mdl-card--border">
    <a class="wiki-link" href="{{link}}"class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">

      <button style="color:#000000;" class="mdl-button mdl-js-button mdl-button--primary">
          Read More
</button>
    </a>
  </div>
  </div>
</div>
  </article>
  {{/each}}
</section>

</main>
