<main>

<section id="search-section">
  <form class="form" action="/wiki" method="get">
    <div class="mdl-textfield mdl-js-textfield form-group">
      <label class="mdl-textfield__label" for="search_query">Search Wiki</label>
      <input  class="mdl-textfield__input" type="search" id="search_query" name="search_query">
          </div>
    <button class="mdl-button mdl-js-button mdl-button--raised" type="submit">Search</button>
</form>
</section>
<section id="results">
  {{#each Wikiarticle}}
  <article class="">
    <div class="demo-card-wide mdl-card mdl-shadow--2dp">
  <div class="mdl-card__title">
    <h2 class="mdl-card__title-text"></h2>
  </div>
  <div class="mdl-card__supporting-text">
    Lorem ipsum dolor sit amet, consectetur adipiscing elit.
    Mauris sagittis pellentesque lacus eleifend lacinia...
  </div>
  <div class="mdl-card__actions mdl-card--border">
    <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
    {{title}}
    </a>
  </div>
  <div class="mdl-card__menu">
    <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
      <i class="material-icons">share</i>
    </button>
  </div>
</div>
  </article>
  {{/each}}
</section>

</main>
